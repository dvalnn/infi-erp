{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                ship.id,\n                ship.supplier_id,\n                ship.request_date,\n                ship.quantity,\n                ship.cost\n            FROM shipments as ship\n            JOIN suppliers as sup ON ship.supplier_id = sup.id\n            WHERE raw_material_kind = $1\n                AND request_date > $2\n                AND request_date + sup.delivery_time = $3\n                AND ship.arrival_date IS NULL\n            ",
